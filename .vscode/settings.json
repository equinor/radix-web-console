{
    "editor.insertSpaces": true,
    "editor.rulers": [
        80
    ],
    "editor.tabSize": 2,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules/": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "eslint.enable": true
}
