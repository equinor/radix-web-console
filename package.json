{
  "name": "radix-web-console",
  "description": "Radix Web Console; the web-based GUI to administer Radix applications",
  "version": "4.17.1",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "EXTEND_ESLINT=true vite",
    "dev": "EXTEND_ESLINT=true vite",
    "build": "EXTEND_ESLINT=true tsc --allowSyntheticDefaultImports --skipLibCheck & vite build",
    "test": "EXTEND_ESLINT=true vitest watch",
    "test:no-watch": "EXTEND_ESLINT=true vitest run",
    "test:coverage": "EXTEND_ESLINT=true vitest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-ts": "tsc --noEmit  --skipLibCheck",
    "lint:watch": "eslint -w src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-ts:watch": "tsc --noEmit  --skipLibCheck --watch",
    "lint-strict": "tsc --noEmit  --skipLibCheck --strictNullChecks",
    "deps": "npm run deps:license && npm run deps:stale",
    "deps:license": "node scripts/deps-license-check.js",
    "deps:stale": "node scripts/deps-stale-check.js",
    "apigen:cost": "npx @rtk-query/codegen-openapi ./src/store/configs/cost-openapi-config.cts",
    "apigen:log": "npx @rtk-query/codegen-openapi ./src/store/configs/log-openapi-config.cts",
    "apigen:radix": "npx @rtk-query/codegen-openapi ./src/store/configs/radix-openapi-config.cts",
    "apigen:scan": "npx @rtk-query/codegen-openapi ./src/store/configs/scan-openapi-config.cts",
    "apigen:service-now": "npx @rtk-query/codegen-openapi ./src/store/configs/service-now-openapi-config.cts"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.17.0",
    "@azure/msal-react": "^2.0.22",
    "@equinor/eds-core-react": "^0.39.0",
    "@equinor/eds-icons": "^0.21.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@reduxjs/toolkit": "^2.2.5",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "http-status-codes": "^2.3.0",
    "jdenticon": "^3.3.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.1",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.1",
    "react-router": "^6.26.0",
    "react-router-dom": "^6.26.0",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.5",
    "sanitize.css": "^13.0.0",
    "strip-ansi": "^7.1.0",
    "styled-components": "^6.1.11",
    "vite": "^5.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@remix-run/web-fetch": "^4.4.2",
    "@rtk-query/codegen-openapi": "^1.2.0",
    "@testing-library/react": "^15.0.7",
    "@types/lodash": "^4.17.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "depcheck": "^1.4.7",
    "fetch-mock": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.1.0",
    "license-checker": "^25.0.1",
    "miragejs": "^0.1.48",
    "mock-socket": "^9.3.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.4",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.6.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
