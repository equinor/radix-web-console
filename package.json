{
  "name": "radix-web-console",
  "description": "Radix Web Console; the web-based GUI to administer Radix applications",
  "version": "4.14.0",
  "license": "MIT",
  "scripts": {
    "start": "EXTEND_ESLINT=true react-scripts start",
    "build": "EXTEND_ESLINT=true react-scripts build",
    "test": "EXTEND_ESLINT=true react-scripts test",
    "test:coverage": "EXTEND_ESLINT=true react-scripts test --coverage",
    "test:debug": "EXTEND_ESLINT=true react-scripts --inspect=0.0.0.0 test --runInBand",
    "lint": "eslint src",
    "lint:watch": "esw -w src",
    "deps": "npm run deps:license && npm run deps:stale",
    "deps:license": "node scripts/deps-license-check.js",
    "deps:stale": "node scripts/deps-stale-check.js",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.36.0",
    "@azure/msal-react": "^1.5.6",
    "@equinor/eds-core-react": "^0.31.1",
    "@equinor/eds-icons": "^0.19.1",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@react-rxjs/core": "^0.10.4",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.3.6",
    "clsx": "^1.2.1",
    "connected-react-router": "^6.9.3",
    "date-fns": "^2.29.3",
    "history": "^4.10.1",
    "immutability-helper": "^3.1.1",
    "jdenticon": "^3.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nanoid": "^4.0.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-charts": "^4.0.0",
    "react-markdown": "^8.0.7",
    "react-redux": "^7.2.9",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.2",
    "react-toastify": "^9.1.2",
    "redux-saga": "^1.2.3",
    "rxjs": "^7.8.0",
    "sanitize.css": "^13.0.0",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/lodash": "^4.14.194",
    "@types/microsoft-graph": "^2.32.0",
    "@types/react": "^17.0.58",
    "@types/react-dom": "^17.0.19",
    "@types/react-router-dom": "^5.3.3",
    "depcheck": "^1.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "fetch-mock": "^9.11.0",
    "license-checker": "^25.0.1",
    "miragejs": "^0.1.47",
    "mock-socket": "^9.2.1",
    "prettier": "^2.8.8",
    "redux-saga-test-plan": "^4.0.6",
    "swagger-proptypes": "^7.0.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
