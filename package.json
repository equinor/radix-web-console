{
  "name": "radix-web-console",
  "description": "Radix Web Console; the web-based GUI to administer Radix applications",
  "version": "4.16.0",
  "license": "MIT",
  "scripts": {
    "start": "EXTEND_ESLINT=true vite",
    "dev": "EXTEND_ESLINT=true vite",
    "build": "EXTEND_ESLINT=true tsc --allowSyntheticDefaultImports & vite build",
    "test": "EXTEND_ESLINT=true vitest watch",
    "test:no-watch": "EXTEND_ESLINT=true vitest run",
    "test:coverage": "EXTEND_ESLINT=true vitest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:watch": "eslint -w src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deps": "npm run deps:license && npm run deps:stale",
    "deps:license": "node scripts/deps-license-check.js",
    "deps:stale": "node scripts/deps-stale-check.js",
    "test:debug": "EXTEND_ESLINT=true react-scripts --inspect=0.0.0.0 test --runInBand",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.37.1",
    "@azure/msal-react": "^1.5.8",
    "@equinor/eds-core-react": "^0.31.1",
    "@equinor/eds-icons": "^0.19.1",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@react-rxjs/core": "^0.10.4",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "connected-react-router": "^6.9.3",
    "date-fns": "^2.30.0",
    "history": "^4.10.1",
    "immutability-helper": "^3.1.1",
    "jdenticon": "^3.2.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-charts": "^4.0.0",
    "react-markdown": "^8.0.7",
    "react-redux": "^7.2.9",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-select": "^5.7.3",
    "react-toastify": "^9.1.3",
    "redux-saga": "^1.2.3",
    "rxjs": "^7.8.1",
    "sanitize.css": "^13.0.0",
    "styled-components": "^5.3.11",
    "vite": "^4.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@testing-library/react": "^12.1.5",
    "@types/lodash": "^4.14.195",
    "@types/microsoft-graph": "^2.34.1",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "fetch-mock": "^9.11.0",
    "jsdom": "^22.1.0",
    "license-checker": "^25.0.1",
    "miragejs": "^0.1.47",
    "mock-socket": "^9.2.1",
    "prettier": "^2.8.8",
    "redux-saga-test-plan": "^4.0.6",
    "swagger-proptypes": "^7.0.1",
    "typescript": "^5.1.6",
    "vitest": "^0.32.4"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
