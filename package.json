{
  "name": "radix-web-console",
  "description": "Radix Web Console; the web-based GUI to administer Radix applications",
  "version": "4.14.0",
  "license": "MIT",
  "scripts": {
    "start": "EXTEND_ESLINT=true react-scripts start",
    "build": "EXTEND_ESLINT=true react-scripts build",
    "test": "EXTEND_ESLINT=true react-scripts test",
    "test:coverage": "EXTEND_ESLINT=true react-scripts test --coverage",
    "test:debug": "EXTEND_ESLINT=true react-scripts --inspect=0.0.0.0 test --runInBand",
    "lint": "eslint src",
    "lint:watch": "esw -w src",
    "deps": "npm run deps:license && npm run deps:stale",
    "deps:license": "node scripts/deps-license-check.js",
    "deps:stale": "node scripts/deps-stale-check.js",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.28.1",
    "@azure/msal-react": "^1.4.5",
    "@equinor/eds-core-react": "^0.20.4",
    "@equinor/eds-icons": "^0.12.0",
    "@microsoft/microsoft-graph-client": "^3.0.2",
    "@react-rxjs/core": "^0.9.5",
    "@reduxjs/toolkit": "^1.8.0",
    "classnames": "^2.3.1",
    "connected-react-router": "^6.9.1",
    "date-fns": "^2.25.0",
    "history": "^4.10.1",
    "immutability-helper": "^3.1.1",
    "jdenticon": "^3.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "nanoid": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-charts": "^3.0.15",
    "react-markdown": "^8.0.3",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.0",
    "react-select": "^5.4.0",
    "redux-saga": "^1.1.3",
    "rxjs": "^7.5.5",
    "sanitize.css": "^13.0.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/lodash": "^4.14.182",
    "@types/microsoft-graph": "^2.22.0",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-router-dom": "^5.3.3",
    "depcheck": "^1.4.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fetch-mock": "^9.11.0",
    "license-checker": "^25.0.1",
    "miragejs": "^0.1.43",
    "mock-socket": "^9.1.2",
    "prettier": "^2.5.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga-test-plan": "^4.0.4",
    "swagger-proptypes": "^7.0.1",
    "typescript": "^4.6.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
